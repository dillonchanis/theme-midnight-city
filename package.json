{
    "name": "midnight-city",
    "displayName": "Midnight City",
    "description": "A cool dark outrun style theme.",
    "version": "0.0.1",
    "publisher": "Dillon Chanis",
    "scripts": {
		"dev": "node ./scripts/dev.js",
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"vscode:prepublish": "npm run build"
	},
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
			{
				"label": "Midnight City",
				"uiTheme": "vs-dark",
				"path": "./theme/midnight-city.json"
			},
			{
				"label": "Midnight City Soft",
				"uiTheme": "vs-dark",
				"path": "./theme/midnight-city-soft.json"
			}
		]
    },
    "devDependencies": {
        "js-yaml": "3.13.0",
        "prettier": "1.16.4",
        "tinycolor2": "1.4.1"
    }
}
